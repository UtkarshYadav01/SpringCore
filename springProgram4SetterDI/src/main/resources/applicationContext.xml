<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.ucode.beans.Address" id="AddrId">
        <property name="houseNo" value="64"/>
        <property name="city" value="Agra"/>
        <property name="pinCode" value="282001"/>
    </bean>

    <bean class="com.ucode.beans.Student" id="stdId">
        <property name="rollNo" value="101"/>
        <property name="name" value="Utkarsh"/>
        <property name="address" ref="AddrId"/>
    </bean>


</beans>